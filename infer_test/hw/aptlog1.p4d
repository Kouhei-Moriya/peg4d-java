
File
	= LOG+

Chunk
	= LOG

EOL
	= ('\r'? '\n' / !. )

_
	= SPACE*

SPACE
	= [ \t\u000B\u000C]

LOG
	= DAY _ TIME _ ARGS _ ARGS _ ARGS ( _ ARGS)? _ EOL

DAY
	= ( [1-9] [0-9] [0-9] [0-9] ) '-' ( '1' [0-2] / '0' [1-9] ) '-' ( '3' [01] / [12] [0-9] / '0' [1-9] )

TIME
	= ( '2' [0-3] / [01] [0-9] ) ':' [0-5] [0-9] ':' [0-5] [0-9]

ARGS
	= [-0-9A-Za-z_:.+~]+
	/ '\<none\>'
	/ '\<なし\>'

